#processor block decomposition
sms(32,32,32)
#Always specify blocks in block base numbering
blk(on,0:31,0:31,0:31)
#number of zones per cpu
numzones(12,12,12)
#boundary tag information
tag("xMinFaces",face,(0:0,0:32,0:32))
tag("xMaxFaces",face,(32:32,0:32,0:32))
tag("yMinFaces",face,(0:32,0:0,0:32))
tag("yMaxFaces",face,(0:32,32:32,0:32))
tag("zMinFaces",face,(0:32,0:32,0:0))
tag("zMaxFaces",face,(0:32,0:32,32:32))
#Hex subdivisions
sub(10%,0:31, 0:31, 0:31,(7,0,0,0)) #7 hex
seed(10)
